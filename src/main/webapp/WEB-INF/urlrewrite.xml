<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
    <rule>
        <from>^/(.*)login.html</from>
        <to type="redirect">/AMManage/common/login.htm</to>
    </rule>

    <rule>
        <from>^login$</from>
        <to type="redirect">/common/login.htm</to>
    </rule>
    <rule>
        <from>^/manager/login.htm$</from>
        <to type="redirect">/AMManage/common/login.htm</to>
    </rule>
    <rule>
        <from>^/(.*)common/redirectLogin.html</from>
        <to type="redirect">/AMManage/common/login.htm</to>
    </rule>
</urlrewrite>